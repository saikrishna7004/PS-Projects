class Solution(object):
    def answerQueries(self, nums, queries):
        """
        :type nums: List[int]
        :type queries: List[int]
        :rtype: List[int]
        """
        nums.sort()
        answer = [0]*len(nums)
        for i in range(len(queries)):
            t = nums
            while sum(t)>queries[i]:
                t.pop()
            answer[i] = len(t)
        return answer
